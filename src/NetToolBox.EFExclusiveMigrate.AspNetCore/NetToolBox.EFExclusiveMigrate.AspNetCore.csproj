<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <OutputType>Library</OutputType>
    <nullable>enable</nullable>
    <RepositoryUrl>https://github.com/npnelson/EFExclusiveMigrate</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>Ensure an exclusive EF Migration by taking exclusive lock on database</Description>
    <Authors>npnelson</Authors>
    <IsPackable>true</IsPackable>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\NetToolBox.EFExclusiveMigrate.Core\NetToolBox.EFExclusiveMigrate.Core.csproj" />
  </ItemGroup>

</Project>
